{
  "$schema": "http://json-schema.org/draft-07/schema
  "title": "RLBot Command Schema",
  "type": "object",
  "required": ["action", "data"],
  "properties": {
    "action": {
      "type": "string",
      "enum": ["moveAndClick", "camera_rotate", "camera_zoom", "pressKey", "interfaceAction"]
    },
    "data": {
      "type": "object",
      "properties": {
        "targetType": {
          "type": "string",
          "enum": ["npc", "coordinates", "ground_item", "object"]
        },
        "action": {
          "type": "string"
        },
        "npcId": {
          "type": "integer"
        },
        "x": {
          "type": "integer"
        },
        "y": {
          "type": "integer"
        },
        "right": {
          "type": "boolean"
        },
        "degrees": {
          "type": "integer"
        },
        "in": {
          "type": "boolean"
        },
        "key": {
          "type": "string"
        },
        "interfaceId": {
          "type": "integer"
        },
        "groupId": {
          "type": "integer"
        },
        "optionText": {
          "type": "string"
        },
        "objectId": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        }
      }
    }
  }
} 